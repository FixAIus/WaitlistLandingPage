generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("NEXT_DATABASE_URL")
  directUrl = env("NEXT_DIRECT_URL")
}

model Waitlist {
  id                    String          @id @default(uuid())
  clerk_id              String?         @unique
  email                 String          @unique
  first_name            String?
  full_name             String?
  last_name             String?
  instagram_username    String?
  interest_reason       String?         // What makes you interested in an AI appointment setter?
  planned_usage         String?         // What do you plan on using it for?
  business_instagram    String?  
  createdAt             DateTime        @default(now())
  updatedAt             DateTime        @updatedAt
}

model Users {
  id                    String          @id @default(uuid())
  clerk_id              String?         @unique
  email                 String          @unique
  first_name            String?
  full_name             String?
  last_name             String?
  profile_image_url     String?
  createdAt             DateTime        @default(now())
  updatedAt             DateTime        @updatedAt
}
